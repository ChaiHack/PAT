#include<iostream>
#include<vector>
#include<algorithm>
int main()
{
  using namespace std;
  int number[10];
  for (int i = 0; i < 10; i++)
  {
	cin >> number[i];
  }
  for (int i = 1; i < 10; i++)
  {
	if (number[i] != 0)
	{
	  cout << i;
	  number[i]--;
	  break;
	}
  }
  for (int i = 0; i < 10; i++)
  {
	while (number[i] != 0)
	{
	  cout << i;
	  number[i]--;
	}
  }
  return 0;
}
